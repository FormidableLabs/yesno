{
  "name": "yesno-http",
  "version": "0.0.2",
  "description": "Easy HTTP testing",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "yesno": "dist/scripts/cli.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "preversion": "npm run clean && npm run lint && npm run tests",
    "version": "npm run compile",
    "clean": "rimraf dist",
    "compile": "tsc",
    "watch": "npm run compile -- -w",
    "test": "npm run lint && npm run coverage",
    "coverage": "nyc --reporter=html -- npm run tests",
    "tests": "npm run unit && npm run integration",
    "lint": "tslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "unit": "mocha --timeout 2000 --recursive 'test/unit/**/*.spec.ts'",
    "integration": "mocha --timeout 5000 --recursive 'test/integration/**/*.spec.ts'",
    "check": "npm run lint && npm run tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formidablelabs/yesno.git"
  },
  "author": "Ian Walker-Sperber",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/formidablelabs/yesno/issues"
  },
  "homepage": "https://github.com/formidablelabs/yesno#readme",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.4",
    "@types/request-promise": "^4.1.42",
    "@types/sinon": "^5.0.1",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "express": "^4.16.3",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "rimraf": "^2.6.2",
    "sinon": "^6.1.5",
    "source-map-support": "^0.5.8",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "@types/debug": "0.0.30",
    "@types/lodash": "^4.14.116",
    "@types/uuid": "^3.4.4",
    "@types/yargs": "^12.0.1",
    "debug": "^4.0.1",
    "io-ts": "^1.3.4",
    "io-ts-reporters": "0.0.21",
    "lodash": "^4.17.11",
    "mitm": "^1.4.0",
    "readable-stream": "^3.0.6",
    "uuid": "^3.3.2",
    "yargs": "^12.0.2"
  }
}
